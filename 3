if (A[i]%2!=0){
d=A[i];}
}
for (i=0;i<n;i++){
if (A[i]%2!=0){
A[i]= A[i]+d;
}
}
for (i=0;i<n;i++){cout<<A[i]<<" "<<ends;}
return 0;
} 
