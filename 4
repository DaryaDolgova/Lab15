#include <iostream> 
#include <cmath> 
using namespace std; 

int main() 
{ 
int i,n,d,min,max,mi,ma; 
cout<<"N= "; cin>>n;
int *A=new int [n];
for (i=0;i<n;i++) {cin>>A[i];}
min=A[0]; mi=0;
for (i=1;i<n;i++) {
if(A[i]<min){
min=A[i];
mi=i;
}
}
max=A[0]; ma=0;
for (i=1;i<n;i++) {
if(A[i]>max){
max=A[i];
ma=i;
}
}
if (mi<ma){
for (i=mi;i<=ma;i++){
A[i]=0;
}
} else {
for (i=ma;i<=mi;i++){
A[i]=0;
}
}
for (i=0;i<n;i++){cout<<A[i]<<" "<<ends;}
return 0;
}
